<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ResumoPagamentoController">
   <center>
      <VBox prefHeight="317.0" prefWidth="432.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tv_pagamento" prefHeight="323.0" prefWidth="313.0">
              <columns>
                <TableColumn fx:id="tc_pagamentoId" prefWidth="26.0" text="ID" />
                  <TableColumn fx:id="tc_pagamentoDesc" prefWidth="82.0" text="Descrição" />
                <TableColumn fx:id="tc_pagamentoFunc" prefWidth="88.0" text="Funcionário" />
                  <TableColumn fx:id="tc_pagamentoPag" prefWidth="82.0" text="Pagamento" />
                  <TableColumn fx:id="tc_pagamentoData" prefWidth="87.0" text="Data" />
                  <TableColumn fx:id="tc_pagamentoValor" prefWidth="66.0" text="Valor" />
              </columns>
            </TableView>
            <HBox alignment="CENTER_RIGHT" prefHeight="21.0" prefWidth="313.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#act_Fechar" text="Fechar" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
